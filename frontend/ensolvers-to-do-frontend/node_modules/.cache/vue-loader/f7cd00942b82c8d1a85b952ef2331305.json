is.newLabel !== this.label) {\n        this.$emit(\"item-edited\", this.newLabel);\n      }\n    },\n    onCancel() {\n      this.$emit(\"edit-cancelled\");\n    }\n  }\n};\n</script>\n\n\n<style scoped>\n.edit-label {\n  font-family: Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  color: #0b0c0c;\n  display: block;\n  margin-bottom: 5px;\n}\ninput {\n  display: inline-block;\n  margin-top: 0.4rem;\n  width: 100%;\n  min-height: 4.4rem;\n  padding: 0.4rem 0.8rem;\n  border: 2px solid #565656;\n}\nform {\n  display: flex;\n  flex-direction: row;\n  flex-wrap: wrap;\n}\nform > * {\n  flex: 0 0 100%;\n}\n</style>\n"]}]}